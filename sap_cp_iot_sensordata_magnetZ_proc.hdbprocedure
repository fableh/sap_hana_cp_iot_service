PROCEDURE "FABIAN"."development.iot::sap_cp_iot_sensordata_magnetZ_proc" ( 
IN row "development.iot::sap_cp_iot_sensordata.tt_detailsMagnetometerZ",
OUT error "development.iot::sap_cp_iot_sensordata.tt_error" )

LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
DEFAULT SCHEMA FABIAN
AS
BEGIN
 /*****************************
	Write your procedure logic
 *****************************/

 declare lv_measunit string;
 declare lv_meastimestamp string;
  declare lv_MagnetometerZ string;
 declare lv_measuid string;
 declare lv_nodeid string;
 declare lv_macaddress string;
  declare lv_profileid string;
 declare lv_sensorid string;
 
 
 

 
 select MEASUNIT, MEASTIMESTAMP, MAGNETOMETERZ, MEASUID, NODEID, MACADDRESS, PROFILEID, SENSORID  into lv_measunit, lv_meastimestamp, lv_MagnetometerZ, lv_measuid, lv_nodeid, lv_macaddress , lv_profileid, lv_sensorid  from :row;

 if :lv_MagnetometerZ = ' ' then
    error = select 400 as http_status_code,
	    'invalid date' as error_message,
		    'Invalid response from sensor' as detail from dummy;
 else
insert into "FABIAN"."development.iot::sap_cp_iot_sensordata.iotDataMagnetometerZ" values 
("FABIAN"."development.iot::sap_cp_iot_sensordata_magnetZ_id_seq".NEXTVAL, now(),
lv_measunit,
CAST(lv_meastimestamp AS bigint),
CAST(lv_MagnetometerZ AS double),
lv_measuid,
CAST(lv_nodeid AS INTEGER ),
lv_macaddress,
CAST(lv_profileid AS INTEGER),
CAST (lv_sensorid AS INTEGER));

end if;

END;
